@model Festival.Web.Areas.Admin.ViewModels.TransferReservation.DetailTransferReservationVM;
@{
    ViewData["Title"] = "Detail";
}


<div class="col-6">
    <h2 class="detailName">Transfer Service</h2>
    <div class="form-group row detailRow">
        <label class="detailLabel" for="AttendeeName">Attendee : </label>
        <span class="detailData">@Model.AttendeeName</span>
    </div>
    <div class="form-group row detailRow">
        <label class="detailLabel" for="AttendeeEmail">Email : </label>
        <span class="detailData">@Model.AttendeeEmail</span>
    </div>
    <div class="form-group row detailRow">
        <label class="detailLabel" for="Date">Date of Transfer Service : </label>
        <span class="detailData">@Model.DateOfService</span>
    </div>
    <div class="form-group row detailRow">
        <label class="detailLabel" for="AvailableSeats">Vehicle : </label>
        <span style="" class="detailData">@Model.VehicleName</span>
    </div>
    <div class="form-group row detailRow">
        <label class="detailLabel" for="MeetingPoint">Meeting Point : </label>
        <span style="" class="detailData">@Model.MeetingPoint</span>
    </div>
    <div class="detailButtons btn-group">
        <a style="margin-right:5px;" asp-controller="TransferReservation" asp-action="List" class="btn btn-info active" role="button" aria-pressed="true">
            Back
            <i class="fa fa-undo"></i>
        </a>
        <a style="margin-right:5px;" asp-controller="TransferReservation" asp-action="Edit" asp-route-id="@Model.ID" class="btn btn-secondary active" role="button" aria-pressed="true">
            Edit
            <i class="fa fa-edit"></i>
        </a>
        <a asp-controller="TransferReservation" asp-action="Delete" asp-route-id="@Model.ID" class="btn btn-danger" role="button" aria-pressed="true">
            Delete
            <i class="fa fa-trash"></i>
        </a>
    </div>
</div>
<div class="clearfix"></div>
